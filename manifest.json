{
	"manifest_version": 3,
	"name": "Chesstention",
	"description":  "Chesstention: play chess locally, vs computer, or with friends on LAN. Clean UI, themes, and the best way to rizz and aura farm over the board. Chess pieces from Lichess Maestro set (AGPL-3.0).",
	"version": "1.0",
	"action": {
	  "default_popup": "popup.html",
	  "default_icon": {
		"16": "images/New Project (2).png",
		"48": "images/New Project (2).png",
		"128": "images/New Project (2).png"
	  }
	},
	"icons": {
	  "16": "images/New Project (2).png",
	  "48": "images/New Project (2).png",
	  "128": "images/New Project (2).png"
	},
	"permissions": [
	  "storage"
	],
	"host_permissions": [
	  "https://web-production-e734b.up.railway.app/*"
	],
	"web_accessible_resources": [
	  {
		"resources": ["images/*", "media/*"],
		"matches": ["<all_urls>"]
	  }
	]
}
  